{
    "name": "bookingsync/oauth2-bookingsync-php",
    "description": "BookingSync OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien Grosjean",
            "email": "dev@bookingsync.com",
            "homepage": "https://github.com/bookingsync",
            "role": "Developer"
        }
    ],
    "version": "0.1.1",
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "bookingsync"
    ],
    "require": {
        "php": ">=5.4.0",
        "league/oauth2-client": ">=0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "jakub-onderka/php-parallel-lint": "0.8.*"
    },
    "autoload": {
        "psr-4": {
            "Bookingsync\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bookingsync\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
